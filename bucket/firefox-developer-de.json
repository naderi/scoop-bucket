            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/devedition/releases/$version/win32/de-DE/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/devedition/releases/$version/SHA512SUMS",
