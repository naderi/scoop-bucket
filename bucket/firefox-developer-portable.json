        "32bit": {
            "url": "https://archive.mozilla.org/pub/devedition/releases/100.0b7/win32/en-US/Firefox%20Setup%20100.0b7.exe#/dl.7z",
            "hash": "sha512:d43f6cbcb42825ce766ba298df567a049a9fbeb55f8cb451c03d3388c61bcb518fcac0a97f2e2846873512f81580403a7ed6683e8d1d368aa0c5da015a1da33a"
