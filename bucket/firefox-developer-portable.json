        [
            "firefox.exe",
            "Firefox Developer",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_developer\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
