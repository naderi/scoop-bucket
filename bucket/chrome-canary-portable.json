            "hash": "ef5296d8085e8eb643791dd81e609bd566d83e08e61ad0b32575c0a14c96fd6c"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/adf5dzpkcickefzd7o4t3g5c3ycq_101.0.4936.0/101.0.4936.0_chrome_installer.exe#/dl.7z",
