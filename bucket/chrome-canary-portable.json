            "hash": "9441dbe63bffa2d8b129a82e52c5659b1baf424d8a1ad5bc069d64ce5d69ef94"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/hhsqu4fyml5zjfwlvjl6bdsb3u_104.0.5095.0/104.0.5095.0_chrome_installer.exe#/dl.7z",
