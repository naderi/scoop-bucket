            "hash": "ec7de8faf5951a30c4e7a3af89e22ffbeab71b0bf8f41d6ebaefa3ee14ea9e48"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/dc3ddtff7shijr7lr4a7p5egu4_124.0.6366.2/124.0.6366.2_chrome_installer.exe#/dl.7z",
