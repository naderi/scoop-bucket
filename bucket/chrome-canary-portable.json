            "hash": "e42b271b1a3d5f426c6cacf928642498708e8b6e2b3706285610ae65b1ad5487"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/ex5xjc5a5djiut5gxdbuu2kddy_131.0.6778.4/131.0.6778.4_chrome_installer.exe#/dl.7z",
