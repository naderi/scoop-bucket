        [
            "firefox.exe",
            "firefox-developer-fa",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Developer (fa)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "profile",
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_developer\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
