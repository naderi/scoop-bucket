{
    "version": "112.0.1",
    "description": "Popular open source web browser.",
    "homepage": "https://www.mozilla.org/firefox/",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/112.0.1/win64/fa-IR/Firefox%20Setup%20112.0.1.exe#/dl.7z",
            "hash": "sha512:2da0de7baeb9fecafd256e85ee716c9672478601cbfd439aa4ee0d016c1fcaf3205db062c5f3f8ab2cfbb55a6948db25fa5ff4489f1a0ef2e3ffcf534e84d310"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/112.0.1/win32/fa-IR/Firefox%20Setup%20112.0.1.exe#/dl.7z",
            "hash": "sha512:2da0de7baeb9fecafd256e85ee716c9672478601cbfd439aa4ee0d016c1fcaf3205db062c5f3f8ab2cfbb55a6948db25fa5ff4489f1a0ef2e3ffcf534e84d310"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-fa",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox (fa)",
            "-profile \"$dir\\profile\""
        ]
    ],
    "persist": "Profile",
    "checkver": {
        "url": "https://www.mozilla.org/fa-IR/firefox/notes/",
        "regex": "<div class=\"c-release-version\">([\\d.]+)</div>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/fa-IR/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/fa-IR/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/fa-IR/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/fa-IR/$basename"
                }
            }
        }
    }
}
